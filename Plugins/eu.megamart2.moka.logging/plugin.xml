<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.papyrus.moka.service.service">
      <service
            class="eu.megamart2.moka.logging.services.MokaNodeInformationListener"
            description="MegaMark2 Service">
      </service>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="eu.megamart2.moka.logging.category.Megamart2"
            name="%category.name">
      </category>
      <view
            category="eu.megamart2.moka.logging.category.Megamart2"
            class="eu.megamart2.moka.logging.ui.MegamartView"
            id="eu.megamart2.moka.logging.view1"
            name="%view.name"
            restorable="true">
      </view>
   </extension>

</plugin>
